num = int(input("Dame un numero de mes: "))

meses = ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio",
         "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"]

dias = ["31", "28", "31", "30", "31", "30",
        "31", "31", "30", "31", "30", "31"]

print("Mes: ", meses [num -1])
print("Dias: ", dias [num -1])
